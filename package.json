{
  "name": "wishlist",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "mocked": "ng serve --proxy-config ./proxy.config.json & nodemon mock-server.js",
    "build": "ng build",
    "prod": "ng build --prod",
    "docker-build": "eval $(minikube docker-env); docker build -t frontend:latest .",
    "kube-apply": "kubectl apply -n prod -f kubernetes/frontend.yml",
    "kube-delete": "kubectl delete -n prod -f kubernetes/frontend.yml",
    "kube-deploy": "npm run prod && npm run docker-build && npm run kube-apply",
    "kube-delete-deploy": "npm run kube-delete && npm run kube-deploy",
    "test": "ng test",
    "test-headless": "ng test --browsers ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e --proxy-config proxy.config.json",
    "e2e-headless": "ng e2e --config e2e/config/protractor.e2e.headless.conf.js --proxy-config proxy.config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.7",
    "@angular/cdk": "5.2.4",
    "@angular/common": "^5.2.7",
    "@angular/compiler": "^5.2.7",
    "@angular/core": "^5.2.7",
    "@angular/forms": "^5.2.7",
    "@angular/http": "^5.2.7",
    "@angular/material": "5.2.4",
    "@angular/platform-browser": "^5.2.7",
    "@angular/platform-browser-dynamic": "^5.2.7",
    "@angular/router": "^5.2.7",
    "@ngrx/effects": "^5.1.0",
    "@ngrx/router-store": "^5.1.0",
    "@ngrx/store": "^5.1.0",
    "core-js": "^2.5.3",
    "hammerjs": "^2.0.8",
    "ngrx-store-localstorage": "^5.0.0",
    "rxjs": "^5.5.6",
    "typescript": "^2.7.2",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.7.2",
    "@angular/compiler-cli": "^5.2.7",
    "@ngrx/store-devtools": "^5.1.0",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.8.6",
    "@types/node": "~9.4.6",
    "codelyzer": "~4.2.1",
    "express": "^4.16.3",
    "fs-extra": "^4.0.1",
    "jasmine-core": "~2.9.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-htmlfile-reporter": "~0.3",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "ngrx-store-freeze": "^0.1.6",
    "ngrx-store-logger": "^0.2.0",
    "nodemon": "^1.17.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "body-parser": "latest"
  }
}
