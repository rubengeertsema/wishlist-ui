{
  "name": "wishlist",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "mocked": "ng serve --proxy-config ./proxy.config.json & nodemon mock-server.js",
    "build": "ng build",
    "prod": "ng build --prod",
    "docker-build": "eval $(minikube docker-env); docker build -t frontend:latest .",
    "kube-apply": "kubectl apply -n prod -f kubernetes/frontend.yml",
    "kube-delete": "kubectl delete -n prod -f kubernetes/frontend.yml",
    "kube-deploy": "npm run prod && npm run docker-build && npm run kube-apply",
    "kube-delete-deploy": "npm run kube-delete && npm run kube-deploy",
    "test": "ng test",
    "test-headless": "ng test --browsers ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e --proxy-config proxy.config.json",
    "e2e-headless": "ng e2e --config e2e/config/protractor.e2e.headless.conf.js --proxy-config proxy.config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/cdk": "7.2.0",
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/forms": "^7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/material": "7.2.0",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/router-store": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "core-js": "^2.6.1",
    "hammerjs": "^2.0.8",
    "keycloak-js": "^4.8.2",
    "ngrx-store-localstorage": "^5.1.1",
    "rxjs": "^6.3.3",
    "rxjs-compat": "6.3.3",
    "tslib": "^1.9.0",
    "typescript": "3.1.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "^7.1.4",
    "@angular/compiler-cli": "^7.1.4",
    "@ngrx/store-devtools": "^7.0.0",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "3.3.5",
    "@types/node": "~10.12.18",
    "body-parser": "latest",
    "codelyzer": "~4.5.0",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-htmlfile-reporter": "~0.3",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-junit-reporter": "^1.2.0",
    "ngrx-store-freeze": "^0.2.4",
    "ngrx-store-logger": "^0.2.2",
    "nodemon": "^1.18.9",
    "ts-node": "~7.0.1",
    "tslint": "~5.12.0"
  }
}
